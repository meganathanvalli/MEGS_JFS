package basic;

import java.util.Scanner;

public class User_Input_Sum_Of_Array {

	public static void main(String[] args) {
		
		Scanner s = new Scanner(System.in);
		int[] arr = new int[10];
		int sum = 0;
		
		System.out.print("Enter the Elements: ");
		for(int i=0; i<10; i++)
		{
			arr[i] = s.nextInt();
		}
		
		for(int i=0; i<10; i++) {
			sum+=arr[i];
		}
		
		System.out.print("Sum of an array elements is: "+sum);
		
	}
	
}


		

